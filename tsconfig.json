{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~assets/*": [
        "src/assets/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~stores/*": [
        "src/stores/*"
      ],
      "~style/*": [
        "src/style/*"
      ],
      "~types/*": [
        "src/types/*"
      ],
      "~/*": [
        "src/*"
      ]
    }
  }
}